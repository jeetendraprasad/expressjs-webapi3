{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            //"runtimeExecutable": "${NODE_EXE_PATH}",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/dist/index.js",
            "preLaunchTask": "tsc: build - tsconfig.json",
            "outFiles": [
                "${workspaceFolder}/dist/**/*.js"
            ]
        }
    ]
}